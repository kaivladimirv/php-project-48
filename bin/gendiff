#!/usr/bin/env php
<?php

declare(strict_types=1);

use function Differ\DocReader\readDoc;
use function cli\line;

require __DIR__ . '/../vendor/autoload.php';

$doc = readDoc();
$args = Docopt::handle($doc);

$diff = Differ\Differ\genDiff($args['<firstFile>'], $args['<secondFile>']);

line($diff);
